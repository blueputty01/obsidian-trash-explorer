		"@types/node": "^20.14.9",
		"@typescript-eslint/eslint-plugin": "^7.14.1",
		"@typescript-eslint/parser": "^7.14.1",
		"builtin-modules": "^4.0.0",
		"esbuild": "^0.21.5",
		"esbuild-svelte": "^0.8.1",
		"obsidian": "latest",
		"svelte": "^4.2.18",
		"svelte-preprocess": "^6.0.1",
		"tslib": "^2.6.3",
		"typescript": "^5.5.2",
		"vitest": "^1.6.0"
